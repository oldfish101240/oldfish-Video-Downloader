        function sortFormats(formats) {
            const priority = {"å½±ç?+?³è?": 3, "å½±ç?": 2, "?³è?": 1}; // ?ªå?ç´šå?ç¾?
            return (formats || []).slice().sort(function(a, b) {
                let pa = priority[a.desc] || 0;
                let pb = priority[b.desc] || 0;
                if (pa !== pb) return pb - pa; // ?‰æ?è¿°å„ª?ˆç??å?
                return a.value.localeCompare(b.value); // ?è¿°?¸å??‚æ??¼å?æ¯é?åº?
            });
